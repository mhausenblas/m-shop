 {
   "id": "mesos-dns",
   "instances": 1,
   "cpus": 0.2,
   "mem": 50,
   "cmd": "/mesos-dns -config=/config.json",
   "container": {
     "type": "DOCKER",
       "docker": {
         "image": "mesosphere/mesos-dns:latest",
         "network": "HOST"
       },
       "volumes": [
             {
               "containerPath": "/config.json",
               "hostPath": "/etc/mesos-dns/config.js",
               "mode": "RO"
             }
       ]  
   }
}